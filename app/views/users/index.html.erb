<div class="col-xs-3">
  <%= render 'users/info', users:@user %>
  <%= render 'books/new', books:@books %>
</div>

<h1>Users</h1>

<table class="table table-striped">

  <thead>
    <th><p>image</p></th>
    <th><p>name</p></th>
    <th><p>show</p></th>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr class="new-table">
      <td><p><%= image_tag user.get_profile_image(100,100) %></p></td>
      <td><p><%= user.name %></p></td>
      <td><p><%= link_to "Show", user_path(user) %></p></td>

    </tr>

    <% end %>
  </tbody>

</table>

