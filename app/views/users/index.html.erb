<h1>Users</h1>

<table class="table table-striped">
  
  <thead>
    <th><p>image</p></th>
    <th><p>name</p></th>
    <th><p>show</p></th>
  </thead>
  
  <tbody>
    <% @users.each do |user| %>
    <tr class="new-table">
    <td><p><%= user.name %></p></td>
    <td><p><%= link_to "Show", user_path(@user) %></p></td>

    </tr>

    <% end %>
  </tbody>
  
</table>

